    getCountryName(val) {
        let name = '';
        for (let v of this.data.countries) {
            if (val == v.value)
                name = v.name;
        }
        return name;
    }
    getStateName(val, val2) {
        let name = "";
        if (this.data.states[val]) {
            for (let v of this.data.states[val]) {
                if (val2 == v.value)
                    name = v.name;
            }
        }
        else
            name = "other";
        return name;
    }
    getStateCode(c, s) {
        let name = "";
        if (this.data.states[c]) {
            for (let v of this.data.states[c]) {
                if (s == v.name)
                    name = v.value;
            }
        }
        else
            name = "s";
        return name;
    }
    getContientCode(con) {
        return this.data.continent[con];
    }
};
