        this.langId = "1";
        this.loader = "dots";
        this.newProductDuration = 100;
        this.cartButton = 0; //1 = show and 0 = hide
        this.currencyPos = localStorage.currencyPos;
        this.homePage = 9;
        this.categoryPage = 1;
        this.siteUrl = "https://everyday.ug";
        this.appName = "Everyday Shopping";
        this.packgeName = 1;
        this.introPage = 1;
        this.myOrdersPage = 1;
        this.newsPage = 1;
        this.wishListPage = 1;
        this.shippingAddressPage = 1;
        this.aboutUsPage = 1;
        this.contactUsPage = 1;
        this.editProfilePage = 1;
        this.settingPage = 1;
        this.rateApp = 1;
        this.shareApp = 1;
        this.fbButton = 1;
        this.googleButton = 1;
        this.notificationType = "1";
